{
  "extends": "airbnb-base",
  "rules": {
    "strict": [0, "global"],
    "no-console": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "import/no-extraneous-dependencies": [
      "error", {
        "devDependencies": true
      }
    ],
    "space-before-function-paren": ["error", "never"],
    "prefer-rest-params": 0,
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "import/order": ["error", {
      "newlines-between": "always"
    }]
  }
}
